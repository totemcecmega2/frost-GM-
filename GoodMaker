local function QDTZQ_fake_script() -- B1.LocalScript 

	local script = Instance.new('LocalScript', B2)
	

	

	script.Parent.MouseButton1Click:Connect(function() --- when the button is clicked it calls this 

local Text = box.text

game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = Text



	end)

end

